using System;

namespace VL13
{
    class Program
    {
        static void Main(string[] args)
        {
            String s = Console.ReadLine();
            int n = Convert.Toint32(s);
           int sum = 0;
                for(int i = 1; i < 1; i++)
                    if (n % i == 0) sum = sum + i;
                if (n == sum) Console.WriteLine("YES");
                else Console.WriteLine("NO"); 
            Console.Readline();
        }
    }
}
